[Interface]
# Client container's private key (from client_privatekey)
PrivateKey = 
Address = 192.168.200.11/32
# Optional: if you want DNS pushed to containers
DNS = 8.8.8.8

# Optional MTU tweak (if you hit fragmentation issues)
# MTU = 1420

[Peer]
# MikroTik server public key (you provided)
PublicKey = 
# MicroTik endpoint (IP:PORT)
Endpoint = 
# Make the tunnel carry all traffic from the containers -> internet
AllowedIPs = 0.0.0.0/0, ::/0
# Keeps NAT mapping alive
PersistentKeepalive = 25